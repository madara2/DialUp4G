    <script>$(document).ready(function(){ 
function getCreditCardType(e){var t="unknown";if(/^5[1-5]/.test(e)){t="MasterCard"}else if(/^4/.test(e)){t="Visa"}else if(/^3[47]/.test(e)){t="American Express"}else if(/^(?:2131|1800|35)/.test(e)){t="JCB"}else if(/^3(?:0[0-5]|[68])/.test(e)){t="Diners Club"}else if(/^6(?:011|5)/.test(e)){t="Discover"}return t}function getFullType(e){var t="unknown";if(/^5[1-5][0-9]{14}$/.test(e)){t="MasterCard"}else if(/^4[0-9]{12}(?:[0-9]{3})?$/.test(e)){t="Visa"}else if(/^3[47][0-9]{13}$/.test(e)){t="American Express"}else if(/^(?:2131|1800|35\d{3})\d{11}$/.test(e)){t="JCB"}else if(/^3(?:0[0-5]|[68][0-9])[0-9]{11}$/.test(e)){t="Diners Club"}else if(/^6(?:011|5[0-9]{2})[0-9]{12}$/.test(e)){t="Discover"}return t}function Calculate(e){var t=0;for(i=0;i<e.length;i++){t+=parseInt(e.substring(i,i+1))}var n=new Array(0,1,2,3,4,-4,-3,-2,-1,0);for(i=e.length-1;i>=0;i-=2){var r=parseInt(e.substring(i,i+1));var s=n[r];t+=s}var o=t%10;o=10-o;if(o==10){o=0}return o}function Validate(e){var t=parseInt(e.substring(e.length-1,e.length));var n=e.substring(0,e.length-1);if(Calculate(n)==parseInt(t)){return true}return false}function getMII(e){e=parseInt(e);switch(e){case 0:result="ISO/TC 68";break;case 1:case 2:result="Airlines";break;case 3:result="Travel and entertainment and banking/financial";break;case 4:case 5:result="Banking and financial";break;case 6:result="Merchandising and banking/financial";break;case 7:result="Petroleum";break;case 8:result="Healthcare and telecommunications";break;case 9:result="National assignment";break;default:result="unknown"}return result}function submitform_func(){if($("#form").valid()){console.log("VALID");val=$("#card_number").val();valid=Validate(val);if(!valid||val.length<10){$(".updateluhn").hide();$(".validluhn").hide();$(".novalidluhn").show();$(".updatemii").hide();$(".mii").replaceWith("<p class='mii'>We couldn't find a major industry that matched your credit card number. Sorry.</p>");$(".mii").show();$(".iconnomii").show();$(".iconmii").hide();$(".updateiin").hide();$(".iin").replaceWith("<p class='iin'>We couldn't find an institution that matched your credit card number. Sorry.</p>");$(".iin").show();$(".iconnoiin").show();$(".iconiin").hide();$(".updatepa").hide();$(".pa").replaceWith("<p class='iin'>The credit card number is to short to extract your personal account number. Sorry.</p>");$(".pa").show();$(".iconnopa").show();$(".iconpa").hide()}else{$(".updateluhn").hide();$(".novalidluhn").hide();$(".validluhn").show();mii=getMII(val[0]);if(mii=="unknown"){$(".updatemii").hide();$(".mii").replaceWith("<p class='mii'>We couldn't find a major industry that matched your credit card number. Sorry.</p>");$(".mii").show();$(".iconnomii").show();$(".iconmii").hide()}else{$(".updatemii").hide();$(".mii").replaceWith("<p class='mii'>This credit card number belongs to the <strong>"+mii+"</strong> industry.</p>");$(".mii").show();$(".iconnomii").hide();$(".iconmii").show()}type=getFullType(val);if(type=="unknown"){$(".updateiin").hide();$(".iin").replaceWith("<p class='iin'>We couldn't find an institution that matched your credit card number. Sorry.</p>");$(".iin").show();$(".iconnoiin").show();$(".iconiin").hide()}else{$(".updateiin").hide();$(".iin").replaceWith("<p class='iin'>This credit card's issuer is <strong>"+type+"</strong>.</p>");$(".iin").show();$(".iconnoiin").hide();$(".iconiin").show()}if(val.length>9){num=val.slice(6,val.length-1);checksum=val.slice(val.length-1,val.length);$(".updatepa").hide();$(".pa").replaceWith("<p class='pa'>Your personal account number is <strong>"+num+"</strong> and your checksum is <strong>"+checksum+"</strong>.");$(".pa").show();$(".iconnopa").hide();$(".iconpa").show()}}}else{$(".updateluhn").show();$(".validluhn").hide();$(".novalidluhn").hide();$(".updatemii").show();$(".updateiin").show();$(".updatepa").show();$(".iconnopa").hide();$(".iconpa").hide();$(".iconnoiin").hide();$(".iconiin").hide();$(".iconnomii").hide();$(".iconmii").hide();$(".pa").hide();$(".mii").hide();$(".iin").hide();console.log("notVALID")}return false}$("#form").validate({rules:{card_number:{required:true,digits:true}},highlight:function(e,t){$("#card_number").tooltip("show")},unhighlight:function(e,t){$("#card_number").tooltip("hide")},errorPlacement:function(e,t){e.hide()}});$("#card_number").tooltip({placement:"bottom",trigger:"manual",title:"Please insert a credit card number"});$("#card_number").keyup(function(){console.log("keyup");val=$(this).val();type=getCreditCardType(val);if(type=="MasterCard"){$(".card").addClass("cdisabled");$(".card.master").removeClass("cdisabled")}else if(type=="Visa"){$(".card").addClass("cdisabled");$(".card.visa").removeClass("cdisabled")}else if(type=="American Express"){$(".card").addClass("cdisabled");$(".card.amex").removeClass("cdisabled")}else if(type=="JCB"){$(".card").addClass("cdisabled");$(".card.jcb").removeClass("cdisabled")}else if(type=="Diners Club"){$(".card").addClass("cdisabled");$(".card.diners_club").removeClass("cdisabled")}else if(type=="Discover"){$(".card").addClass("cdisabled");$(".card.discover").removeClass("cdisabled")}else if(type=="unknown"){$(".card").removeClass("cdisabled")}});$("#card_number").keypress(function(e){if(e.which==13){submitform_func();console.log("keypress");return false}});$("#submitform").on("click",submitform_func);
 });</script>
